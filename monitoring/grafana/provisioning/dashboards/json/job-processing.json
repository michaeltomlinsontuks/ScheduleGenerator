{
  "dashboard": {
    "id": null,
    "uid": "job-processing",
    "title": "Job Processing",
    "tags": ["production", "jobs", "pdf"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Job Creation Rate",
        "type": "graph",
        "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(pdf_jobs_total[5m])) by (type)",
            "legendFormat": "{{type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "ops", "label": "Jobs/sec" },
          { "format": "short" }
        ]
      },
      {
        "id": 2,
        "title": "Queue Depth",
        "type": "graph",
        "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "queue_jobs_waiting{queue=\"pdf-processing\"}",
            "legendFormat": "Waiting Jobs",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "short", "label": "Jobs" },
          { "format": "short" }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": { "params": [500], "type": "gt" },
              "operator": { "type": "and" },
              "query": { "params": ["A", "5m", "now"] },
              "reducer": { "params": [], "type": "avg" },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "60s",
          "handler": 1,
          "name": "High Queue Depth",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 3,
        "title": "PDF Processing Duration (P50, P95, P99)",
        "type": "graph",
        "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(pdf_processing_duration_seconds_bucket{status=\"completed\"}[5m])) by (le, type))",
            "legendFormat": "P50 - {{type}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(pdf_processing_duration_seconds_bucket{status=\"completed\"}[5m])) by (le, type))",
            "legendFormat": "P95 - {{type}}",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(pdf_processing_duration_seconds_bucket{status=\"completed\"}[5m])) by (le, type))",
            "legendFormat": "P99 - {{type}}",
            "refId": "C"
          }
        ],
        "yaxes": [
          { "format": "s", "label": "Duration" },
          { "format": "short" }
        ]
      },
      {
        "id": 4,
        "title": "Job Success Rate",
        "type": "graph",
        "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(pdf_jobs_total{status=\"completed\"}[5m])) / sum(rate(pdf_jobs_total[5m]))",
            "legendFormat": "Success Rate",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "percentunit", "label": "Success Rate", "min": 0, "max": 1 },
          { "format": "short" }
        ]
      },
      {
        "id": 5,
        "title": "Jobs by Type (Total)",
        "type": "stat",
        "gridPos": { "x": 0, "y": 16, "w": 8, "h": 4 },
        "targets": [
          {
            "expr": "sum(pdf_jobs_total) by (type)",
            "legendFormat": "{{type}}",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "calcs": ["lastNotNull"],
            "fields": "",
            "values": false
          },
          "textMode": "value_and_name"
        }
      },
      {
        "id": 6,
        "title": "Failed Jobs (Last Hour)",
        "type": "stat",
        "gridPos": { "x": 8, "y": 16, "w": 8, "h": 4 },
        "targets": [
          {
            "expr": "sum(increase(pdf_jobs_total{status=\"failed\"}[1h]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "calcs": ["lastNotNull"],
            "fields": "",
            "values": false
          },
          "textMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 10 },
                { "color": "red", "value": 50 }
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Average Processing Time",
        "type": "stat",
        "gridPos": { "x": 16, "y": 16, "w": 8, "h": 4 },
        "targets": [
          {
            "expr": "rate(pdf_processing_duration_seconds_sum{status=\"completed\"}[5m]) / rate(pdf_processing_duration_seconds_count{status=\"completed\"}[5m])",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "calcs": ["lastNotNull"],
            "fields": "",
            "values": false
          },
          "textMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 30 },
                { "color": "red", "value": 60 }
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Job Processing Rate (5m avg)",
        "type": "graph",
        "gridPos": { "x": 0, "y": 20, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(pdf_jobs_total{status=\"completed\"}[5m]))",
            "legendFormat": "Completed",
            "refId": "A"
          },
          {
            "expr": "sum(rate(pdf_jobs_total{status=\"failed\"}[5m]))",
            "legendFormat": "Failed",
            "refId": "B"
          }
        ],
        "yaxes": [
          { "format": "ops", "label": "Jobs/sec" },
          { "format": "short" }
        ]
      },
      {
        "id": 9,
        "title": "Processing Duration Distribution",
        "type": "heatmap",
        "gridPos": { "x": 12, "y": 20, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(pdf_processing_duration_seconds_bucket{status=\"completed\"}[5m])) by (le)",
            "format": "heatmap",
            "legendFormat": "{{le}}",
            "refId": "A"
          }
        ],
        "dataFormat": "tsbuckets"
      }
    ]
  }
}
